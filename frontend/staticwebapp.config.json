{
"navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*", 
      "/*.css", "/*.js",
      "/favicon.ico",
      "/manifest.webmanifest"
    ]
  },

  "globalHeaders": {
    "Strict-Transport-Security":
      "max-age=31536000; includeSubDomains; preload",
    "Content-Security-Policy":
      "default-src 'self'; connect-src 'self' __API_ORIGIN__"
  }
}

